{% extends 'base.html.twig' %}

{% block title %}{% trans %}Edit{% endtrans %}{% endblock %}

{% block content %}
    <div>
        <h1>{% trans %}Company Address{% endtrans %}</h1>

        {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
        {{ form_widget(form) }}

        <div class="form-group">
            <div class="col-sm-10">
                <button type="submit" class="btn btn-dark">{% trans %}Update{% endtrans %}</button>
            </div>
        </div>
        {{ form_end(form) }}
    </div>
{% endblock %}
